  memcpy(ptr, message, messageSizeInBytes);

  myControlData->NumberMessagesInQueue++;

  ++myControlData->CurrentWriteSlot %= maxMsgs;

  myControlData->getMsgSemaphore.post();  // Wake \"timedGetMessage\".

